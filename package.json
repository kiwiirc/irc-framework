{
  "name": "irc-framework",
  "version": "2.10.2",
  "description": "A better IRC framework for node.js",
  "main": "index.js",
  "dependencies": {
    "eventemitter3": "^2.0.2",
    "iconv-lite": "^0.4.11",
    "lodash": "^3.10.1",
    "middleware-handler": "^0.2.0",
    "socksjs": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.4.1",
    "chai-subset": "^1.2.0",
    "istanbul": "^0.4.1",
    "jshint": "^2.8.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
